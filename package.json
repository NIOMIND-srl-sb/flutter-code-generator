{
    "name": "flutter-code-generator",
    "displayName": "Flutter code generator",
    "description": "Flutter code generator",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.78.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.createModel",
        "onCommand:extension.createStyle",
        "onCommand:extension.createApp"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "flutter-code-generator.createModel",
                "title": "Create Model file",
                "category": "Flutter Architecture"
            },
            {
                "command": "flutter-code-generator.createStyle",
                "title": "Create Style file",
                "category": "Flutter Architecture"
            },
            {
                "command": "flutter-code-generator.createApp",
                "title": "Create App file",
                "category": "Flutter Architecture"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "command": "flutter-code-generator.createModel",
                    "group": "1_flutter_code_generatoer"
                },
                {
                    "command": "flutter-code-generator.createStyle",
                    "group": "1_flutter_code_generatoer"
                },
                {
                    "command": "flutter-code-generator.createApp",
                    "group": "1_flutter_code_generatoer"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts"
    },
    "devDependencies": {
        "@types/vscode": "^1.78.0",
        "@types/glob": "^8.1.0",
        "@types/mocha": "^10.0.1",
        "@types/node": "16.x",
        "@types/shelljs": "^0.8.6",
        "@types/lodash": "^4.14.149",
        "@types/js-yaml": "^3.12.1",
        "@typescript-eslint/eslint-plugin": "^5.59.1",
        "@typescript-eslint/parser": "^5.59.1",
        "eslint": "^8.39.0",
        "glob": "^8.1.0",
        "mocha": "^10.2.0",
        "typescript": "^5.0.4",
        "@vscode/test-electron": "^2.3.0"
    },
    "dependencies": {
        "js-yaml": "^3.13.1",
        "lodash": "^4.17.15",
        "shelljs": "^0.8.3"
    }
}
